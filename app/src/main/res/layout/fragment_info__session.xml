<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="fill_parent"
    android:layout_width="fill_parent"
    android:orientation="vertical">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:orientation="vertical"
        android:id="@+id/linearlayoutHR"
        android:visibility="gone"
        tools:context="pt.ua.ieeta.healthintegration.Info_Session$PlaceholderFragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="250sp"
            android:id="@+id/chartHR"
            android:orientation="vertical">
        </LinearLayout>

        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:weightSum="4">
            <TextView
                android:id="@+id/lbl_countHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Count:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_countHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
            <TextView
                android:id="@+id/lbl_sumHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Sum:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_sumHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:weightSum="4">
            <TextView
                android:id="@+id/lbl_minHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Min:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_minHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
            <TextView
                android:id="@+id/lbl_maxHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Max:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_maxHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:weightSum="4">
            <TextView
                android:id="@+id/lbl_avgHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Avg:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_avgHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
            <TextView
                android:id="@+id/lbl_varHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10sp"
                android:text="Variance:"
                android:layout_weight="1"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/value_varHR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>

